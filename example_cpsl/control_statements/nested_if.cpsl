var
i : integer;

begin
read(i);
if i = 42 then
write("The answer to life, the universe, and everything");
elseif i < 10 then
write("Hey, that's a single digit, ");
if i=0 then
write("and that digit is 0!");
elseif i=2|i=4|i=6|i=8 then
write("and that digit is even!");
elseif i=1|i=3|i=5|i=7|i=9 then
write("and that digit is odd!");
end
elseif i > 100 then
write("Woah that's a big number!");
else
write("Well I guess that number isn't particularly interesting to me...");
end
end.

